
@model List<Jogo>
    
@{ 
    ViewData["Title"] = "Jogos Criados -";

}



@if(Model.Count==0)
{

    <h3>Sem Jogos</h3>


}


@foreach(Jogo j in Model)
{


<div class="well">

    <b>ID: </b> @j.ID
    <b>Nome: </b> @j.Nome
    <b>Missão:</b> @j.Missao
    <b>Mísseis: </b> @j.Misseis
    <b>Score: </b> @j.Score
    <b>CoordX: </b> @j.coordx
    <b>CoordY: </b> @j.coordy
   
</div>

}
